<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<array>
	<dict>
		<key>name</key>
		<string>Mock server</string>
		<key>apiUrl</key>
		<string>https://mock.example.com/</string>
		<key>runType</key>
		<string>runQuickly</string>
		<key>networkTimeoutSec</key>
		<integer>4</integer>
	</dict>
	<dict>
		<key>name</key>
		<string>Test server</string>
		<key>apiUrl</key>
		<string>https://test.example.com/</string>
		<key>subConfigs</key>
		<array>
			<dict>
				<key>name</key>
				<string>Test server (insecure)</string>
				<key>acceptAllSSL</key>
				<true/>
			</dict>
		</array>
	</dict>
	<dict>
		<key>name</key>
		<string>Acceptation server</string>
		<key>apiUrl</key>
		<string>https://accept.example.com/</string>
		<key>runType</key>
		<string>runStrictly</string>
	</dict>
	<dict>
		<key>name</key>
		<string>Production</string>
	</dict>
</array>
</plist>
